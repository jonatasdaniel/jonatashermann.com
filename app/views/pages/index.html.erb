<script type="text/javascript">
    $(document).on('submit', '#contactForm', function(e) {
      $(this).find('button[type="submit"]').attr('disabled', true);
      $('#preloader').delay(200).fadeIn('slow');
      $.ajax({
        url: 'send_contact_email',
        dataType: 'JSON',
        type: 'POST',
        data: {
          name: $('#InputName').val(),
          email: $('#InputEmail').val(),
          subject: $('#InputSubject').val(),
          message: $('#message-text').val()
        },
        success: function(data) {
          $('#contactForm button[type="submit"]').removeAttr('disabled');
          $('#preloader').delay(200).fadeOut('slow');
          $('.feedback-form .alert').fadeIn('slow');
          setTimeout(function() {
            $('.feedback-form .alert').fadeOut('slow');
          }, 10000);
        }
      });

      e.preventDefault();
      return false;
    });
    $(document).on('ajax:success', '#contactForm', function() {
      $('#contactForm button[type="submit"]').removeAttr('disabled');
      $('#preloader').delay(200).fadeOut('slow');
    });
</script>